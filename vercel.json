{
    "version": 2,
    "builds": [
      {
        "src": "eLMS-SWE/manage.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "50mb" , "runtime":"python3.9"}
      }
    ],
    "routes": [
     
      {
        "src": "/eLMS-SWE(.*)",
        "dest": "eLMS-SWE/wsgi.py"
      }
    ]}